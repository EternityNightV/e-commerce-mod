// This is your Prisma schema file,

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Store {
  id              String    @id @default(uuid()) @map("_id")
  name            String
  userId          String
  createdAt       DateTime   @default(now())
  updatedAt       DateTime   @updatedAt
}
